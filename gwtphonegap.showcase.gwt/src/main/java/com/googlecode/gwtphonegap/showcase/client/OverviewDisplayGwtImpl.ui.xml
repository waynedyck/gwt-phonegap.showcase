<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:mgwt="urn:import:com.googlecode.mgwt.ui.client.widget">
	
	
	<mgwt:LayoutPanel>
		<mgwt:HeaderPanel>
			<mgwt:center><g:HTML>Modules</g:HTML></mgwt:center>
			<mgwt:right><mgwt:HeaderButton ui:field="aboutButton" text="About"></mgwt:HeaderButton></mgwt:right>
		</mgwt:HeaderPanel>
		<mgwt:ScrollPanel scrollingEnabledX="false">
			<mgwt:CellList ui:field="cellList"></mgwt:CellList>
		
		</mgwt:ScrollPanel>
	
	</mgwt:LayoutPanel>
</ui:UiBinder> 